public class Lab14d
{
	public static void main( String args[] )
	{
		//add test cases
		Scanner keyboard = new Scanner(System.in);
		System.out.println("Enter the length of the list");
		int length = keyboard.nextInt();
		
		System.out.println("Enter the grades, separated by a comma");
		String response = keyboard.next();
		String[] gradesArray = response.split(",");
		int[] gradebook = new int[length];
		int transform = 0;
	    for(int i = 0; i < gradesArray.length; i++){
	    	transform = Integer.valueOf(gradesArray[i]);
	    	gradebook[i] = transform;
	    }		
		
	    Grades test = new Grades(length, gradebook);
	}
}