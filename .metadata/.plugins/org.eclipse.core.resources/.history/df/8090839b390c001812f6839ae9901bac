import static java.lang.System.*;

public class Name
{
	private String name;

	public Name()
	{
		name = "Unknown";
	}

	public Name(String s)
	{
		setName(s);
	}

   public void setName(String s)
   {
	   name = s;
   }

	public String getFirst()
	{
		int spaceIndex = name.indexOf(" ");
		String firstName = name.substring(0, spaceIndex - 1);
		return firstName;
	}

	public String getLast()
	{
		int spaceIndex = name.indexOf(" ");
		String lastName = name.substring(spaceIndex + 1, name.length() - 1);
		return lastName;
	}

 	public String toString()
 	{
 		return "";
	}
}