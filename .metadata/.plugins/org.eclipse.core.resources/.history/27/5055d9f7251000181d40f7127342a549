import static java.lang.System.*;

public class Lab11c
{
   public static void main( String args[] )
   {
	   TriangleThree test = new TriangleThree();
	   
	   test.setTriangle("A", 3);
	   test.toString();
	   System.out.println("\n");
	   
	   test.setTriangle("X", 7);
	   test.toString();
	   System.out.println("\n");
	   
	   test.setTriangle("R", 1);
	   test.toString();
	   System.out.println("\n");
	   
	   test.setTriangle("T", 5);
	   test.toString();
	   System.out.println("\n");
	   
	   test.setTriangle("W", 4);
	   test.toString();
	   System.out.println("\n");
   }
}